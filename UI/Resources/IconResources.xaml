<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <!-- ========================================= -->
    <!-- TOOLBAR ICONS (.ico files for consistency) -->
    <!-- ========================================= -->
    
    <!-- Navigation Icons -->
    <BitmapImage x:Key="Icon_Navigation_ArrowUp" UriSource="/Assets/Icons/arrow-up.ico" />
    <BitmapImage x:Key="Icon_Navigation_Refresh" UriSource="/Assets/Icons/refresh-cw.ico" />
    <BitmapImage x:Key="Icon_Navigation_Undo" UriSource="/Assets/Icons/rotate-ccw.ico" />
    <BitmapImage x:Key="Icon_Navigation_Redo" UriSource="/Assets/Icons/rotate-cw.ico" />
    <BitmapImage x:Key="Icon_Navigation_Settings" UriSource="/Assets/Icons/settings.ico" />
    
    <!-- Panel Icons -->
    <BitmapImage x:Key="Icon_Panel_Left" UriSource="/Assets/Icons/panel-left.ico" />
    <BitmapImage x:Key="Icon_Panel_Right" UriSource="/Assets/Icons/panel-right.ico" />
    
    <!-- Content Icons -->
    <BitmapImage x:Key="Icon_Content_Pin" UriSource="/Assets/Icons/pin.ico" />
    <BitmapImage x:Key="Icon_Content_Star" UriSource="/Assets/Icons/star.ico" />
    <BitmapImage x:Key="Icon_Content_Bookmark" UriSource="/Assets/Icons/bookmark.ico" />
    <BitmapImage x:Key="Icon_Content_Link" UriSource="/Assets/Icons/link.ico" />
    <BitmapImage x:Key="Icon_Content_Todo" UriSource="/Assets/Icons/list.ico" />
    
    <!-- Window Icons -->
    <BitmapImage x:Key="Icon_Window_Main" UriSource="/Assets/Icons/app.ico" />
    
    <!-- ========================================= -->
    <!-- LEGACY ALIASES (for backward compatibility) -->
    <!-- ========================================= -->
    
    <!-- Navigation Icons -->
    <BitmapImage x:Key="ArrowUpIcon" UriSource="/Assets/Icons/arrow-up.ico" />
    <BitmapImage x:Key="RefreshIcon" UriSource="/Assets/Icons/refresh-cw.ico" />
    <BitmapImage x:Key="UndoIcon" UriSource="/Assets/Icons/rotate-ccw.ico" />
    <BitmapImage x:Key="RedoIcon" UriSource="/Assets/Icons/rotate-cw.ico" />
    <BitmapImage x:Key="SettingsIcon" UriSource="/Assets/Icons/settings.ico" />
    
    <!-- Panel Icons -->
    <BitmapImage x:Key="PanelLeftIcon" UriSource="/Assets/Icons/panel-left.ico" />
    <BitmapImage x:Key="PanelRightIcon" UriSource="/Assets/Icons/panel-right.ico" />
    <BitmapImage x:Key="SidebarLeftIcon" UriSource="/Assets/Icons/sidebar-left.ico" />
    <BitmapImage x:Key="SidebarRightIcon" UriSource="/Assets/Icons/sidebar-right.ico" />
    <BitmapImage x:Key="LayoutSidebarIcon" UriSource="/Assets/Icons/layout-sidebar.ico" />
    
    <!-- Content Icons -->
    <BitmapImage x:Key="PinIcon" UriSource="/Assets/Icons/pin.ico" />
    <BitmapImage x:Key="StarIcon" UriSource="/Assets/Icons/star.ico" />
    <BitmapImage x:Key="BookmarkIcon" UriSource="/Assets/Icons/bookmark.ico" />
    <BitmapImage x:Key="LinkIcon" UriSource="/Assets/Icons/link.ico" />
    <BitmapImage x:Key="TodoIcon" UriSource="/Assets/Icons/list.ico" />
    <BitmapImage x:Key="WindowIcon" UriSource="/Assets/Icons/app.ico" />
    
</ResourceDictionary> 